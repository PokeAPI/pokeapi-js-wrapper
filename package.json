{
  "name": "pokeapi-js-wrapper",
  "version": "1.2.4",
  "description": "An API wrapper for PokeAPI",
  "main": "dist/index.js",
  "module": "src/index.js",
  "type": "module",
  "files": [
    "dist/index.js",
    "dist/pokeapi-js-wrapper-sw.js",
    "src/*",
    "index.d.ts"
  ],
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "clean": "rm -rf ./dist/*",
    "doctoc": "doctoc .",
    "test": "nyc --reporter=lcov mocha -r mock-local-storage -r source-map-support/register --inline-diffs --timeout 10000 ./test/test.js",
    "serve": "http-server ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PokeAPI/pokeapi-js-wrapper"
  },
  "keywords": [
    "pokedex",
    "pokemon",
    "nintendo",
    "promise",
    "pokeapi",
    "browser",
    "cache"
  ],
  "author": "Alessandro Pezz√© <igougi.ui@gmail.com> (https://github.com/Naramsim)",
  "contributors": [],
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/PokeAPI/pokeapi-js-wrapper/issues"
  },
  "homepage": "https://github.com/PokeAPI/pokeapi-js-wrapper#readme",
  "types": "./index.d.ts",
  "dependencies": {
    "axios": "^1.6.7",
    "localforage": "^1.10.0"
  },
  "devDependencies": {
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.1",
    "chai-things": "^0.2.0",
    "copy-webpack-plugin": "^12.0.2",
    "doctoc": "^2.2.1",
    "http-server": "^14.1.1",
    "mocha": "^10.3.0",
    "mock-local-storage": "^1.1.24",
    "nyc": "^15.1.0",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  }
}
